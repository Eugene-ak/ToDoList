

<header class="box" id="heading">
  <h1><%= listTitle %></h1>
</header>

<main class="box">
  <% for (object of listItems) { %>
  <section class="item">
    <input type="checkbox" name="checkbox" />
    <p><%= object.item %></p>
    <button class="delete-btn" name="<%= object._id %>" onclick="deleteListItems()">Delete</button>
  </section>
  <% } %>

  <form class="item" method="post">
    <input
      type="text"
      name="newItem"
      placeholder="New Item"
      autocomplete="off"
    />
    <button class="button" type="submit" name="list">+</button>
  </form>
</main>
